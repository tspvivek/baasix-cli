  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "ora": "^8.0.1",
    "prettier": "^3.2.0",
    "prompts": "^2.4.2",
    "zod": "^3.22.0"
  }
