{
  "name": "baasix",
  "version": "0.1.4",
  "type": "module",
  "description": "CLI for Baasix Backend-as-a-Service",
  "module": "dist/index.mjs",
  "main": "./dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tspvivek/baasix.git",
    "directory": "cli"
  },
  "homepage": "https://baasix.com/docs/cli-guide",
  "scripts": {
    "build": "tsup",
    "start": "node ./dist/index.mjs",
